class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        intial = 0
        # running the loop unitl length of total list is not equal to intial ( Main aim is to run the loop to each and every element)
        while len(nums) != intial:
            for i in range(intial+1, len(nums)): 
                # Looping through each and every element and checking the required condition
                if nums[intial]+ nums[i] == target:
                    return list((intial, i))
                continue
                # in the first iteration if all elements are not equal, then looping from 2nd element.
            intial+=1
